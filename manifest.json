{
  "manifest_version": 2,

  "name": "Youtube Manipulator",
  "description": "This extension will manipulate youtube",
  "version": "1.0",
  "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
  "browser_action": {
   
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}